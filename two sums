#include<stdio.h>
int main()
{
    /* code */
    int a[10];
    printf("nums=");
    for(int i=0;i<=5;i++){
    scanf("%d",&a[i]);
    }
    printf("target = ");
    int b;
    scanf("%d",&b);
    for(int i=0;i<=2;i++){
        for(int j=i+1;j<=2;j++){
            if(a[i]+a[j]==b){
                printf("%d,%d",i,j);

            }
        }
    }
    return 0;
}
